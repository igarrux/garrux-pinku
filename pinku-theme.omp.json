{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": "\ue285 ",
    "foreground": "#F6E4FF",
    "background": "transparent"
  },
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "alpine": "üêß \uf300",
            "arch": "üêß \uf303",
            "cache_duration": "none",
            "centos": "üêß \uf303",
            "debian": "üêß \uf306",
            "display_distro_name": false,
            "elementary": "üêß \uf309",
            "fedora": "üêß \uf30a",
            "linux": "üêß",
            "macos": " \uf179",
            "manjaro": "üêß \uf312",
            "mint": "üêß \uf30e",
            "nixos": "üêß \uf313",
            "raspbian": "üêß \uf315",
            "redhat": "üêß \uf316",
            "ubuntu": "üêß \uf31b",
            "windows": " \ue62a"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#300446>\ue0b2</>",
          "template": "{{ .Icon }} ",
          "foreground": "#CC68FB",
          "background": "#300446",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "paused_icon": "\ue602",
            "playing_icon": "\uf04c",
            "stopped_icon": "\uf04d"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#300446>\ue0b2</>",
          "template": " \uf1bc  {{ .Icon }} {{ .Artist }} - {{ .Track }} ",
          "foreground": "#F6E4FF",
          "background": "#300446",
          "type": "spotify",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#300446>\ue0b2</>",
          "template": "{{if .Major}} \ue7ba {{end}}{{.Full }} ",
          "foreground": "#F6E4FF",
          "background": "#300446",
          "type": "react",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#300446>\ue0b2</>",
          "template": " \ue753 {{ .Full }} ",
          "foreground": "#F6E4FF",
          "background": "#300446",
          "type": "angular",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#300446>\ue0b2</>",
          "template": " \ue73d  {{ .Full }} ",
          "foreground": "#F6E4FF",
          "background": "#300446",
          "type": "php",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#300446>\ue0b2</>",
          "template": " \ue235 {{ .Full }} ",
          "foreground": "#F6E4FF",
          "background": "#300446",
          "type": "python",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#300446>\ue0b2</>",
          "template": " \ue791 {{ .Full }}",
          "foreground": "#F6E4FF",
          "background": "#300446",
          "type": "ruby",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#300446>\ue0b2</>",
          "template": " \ue7a8 {{ .Full }}",
          "foreground": "#F6E4FF",
          "background": "#300446",
          "type": "rust",
          "style": "diamond"
        },
        {
          "type": "command",
          "style": "diamond",
          "foreground": "#F6E4FF",
          "background": "#300446",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#300446>\ue0b2</>",
          "template": " \uf308  {{ .Output }} ",
          "properties": {
            "shell": "bash",
            "script": "docker version --format '{{.Client.Version}}'"
          }
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#300446>\ue0b2</>",
          "template": "<#CC68FB> \udb80\udf99 </> {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "#F6E4FF",
          "background": "#300446",
          "type": "node",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#300446>\ue0b2</>",
          "template": "<#CC68FB> \udb81\udfd3  </> {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "foreground": "#F6E4FF",
          "background": "#300446",
          "type": "go",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "mapped_shell_names": {
              "bash": "\uf489",
              "cmd": "\uebc4",
              "fish": "\udb80\ude3a",
              "git-bash": "\ue702",
              "mingw64": "\ue702",
              "pwsh": "\udb82\ude0a",
              "zsh": "\ue711"
            }
          },
          "leading_diamond": "\ue0b2",
          "foreground": "#CC68FB",
          "background": "#F6E4FF",
          "type": "shell",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "<transparent,#fff>\ue0b2</>",
          "template": " <#CC68FB>\ue266</> Ram: {{ round .PhysicalPercentUsed .Precision }}% | {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1000000000.0) }}GB/{{ (div .PhysicalTotalMemory 1000000000.0) }}GB <#CC68FB>\ue266</> ",
          "foreground": "#300446",
          "background": "#F6E4FF",
          "type": "sysinfo",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "roundrock",
            "threshold": 0
          },
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b0",
          "template": " {{ .FormattedMs }} ",
          "foreground": "#F6E4FF",
          "background": "#300446",
          "type": "executiontime",
          "style": "diamond"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "leading_diamond": "<transparent,#F6E4FF>\ue0b0</>",
          "trailing_diamond": "\ue0b0",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#CC68FB",
          "background": "#F6E4FF",
          "type": "git",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .Root }}<#ff0044>{{else}}<#CC68FB>{{ end }}\u256d\u2500 </>",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "3:04 PM"
          },
          "template": "{{ if .Root }} <#ff0044>\ueb46 {{ .CurrentDate | date .Format }}</>{{ else }} <#CC68FB>‚ô• {{ .CurrentDate | date .Format }}</>{{ end }}",
          "type": "time",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#ff0000> \ueb46 Root </>",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "enable_hyperlink": true,
            "folder_separator_icon": " \uf105 ",
            "home_icon": "\ueb06",
            "mapped_locations": {
              "~/dev": "\ueb06 \uf105 \udb84\udcea",
              "~/dev/Android": "\ueb06 \uf105 \udb84\udcea \uf105 \udb80\udc34",
              "~/dev/Backend": "\ueb06 \uf105 \udb84\udcea \uf105 \ueba2",
              "~/dev/Fonts": "\ueb06 \uf105 \udb84\udcea \uf105 \ue659",
              "~/dev/Github": "\ueb06 \uf105 \udb84\udcea \uf105 \udb80\udea4",
              "~/dev/Icons": "\ueb06 \uf105 \udb84\udcea \uf105 \uefa8",
              "~/dev/Ios": "\ueb06 \uf105 \udb84\udcea \uf105 \uf0be",
              "~/dev/Mac": "\ueb06 \uf105 \udb84\udcea \uf105 \udb80\uddc4",
              "~/dev/Npm": "\ueb06 \uf105 \udb84\udcea \uf105 \ue5fa",
              "~/dev/Terminal": "\ueb06 \uf105 \udb84\udcea \uf105 \ue795",
              "~/dev/Web": "\ueb06 \uf105 \udb84\udcea \uf105 \udb81\udd9f",
              "/Volumes/General/Development": "\udb84\udcea"
            },
            "style": "full"
          },
          "type": "path",
          "style": "powerline",
          "templates": [
            "<#F6E4FF> | {{ .Path }}</>"
          ]
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "{{ if .Root }}<#ff0044>{{else}}<#CC68FB>{{ end }}\u2570\u2500</> ",
          "type": "status",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
